{
  "name": "@cewald/nuxt-boilerplate-layer",
  "version": "0.0.0",
  "description": "Personal boilerplate layer for nuxt projects",
  "author": "Ewald <me@ewaldewald.com>",
  "keywords": [
    "nuxt",
    "layer"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cewald/nuxt-boilerplate-layer.git"
  },
  "bugs": {
    "url": "https://github.com/cewald/nuxt-boilerplate-layer/issues"
  },
  "license": "GPL-3.0-only",
  "type": "module",
  "main": "./nuxt.config.ts",
  "files": [
    "app",
    "modules",
    "nuxt.config.ts",
    "vite-env.d.ts",
    "tsconfig.json",
    "tailwind.config.ts",
    "package-lock.json",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "dev": "npm run prepare && nuxi dev playground  --no-fork",
    "dev:build": "nuxi build playground",
    "dev:layer": "npm run prepare && nuxt dev",
    "prepare:husky": "husky || exit 0",
    "prepare": "npm run prepare:husky && nuxt prepare",
    "postinstall": "npm run prepare",
    "build": "nuxt build",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "typecheck": "npx nuxi typecheck"
  },
  "devDependencies": {
